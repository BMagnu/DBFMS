<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<ContextMenu xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.bmagnu.dbfms.gui.CTXMenuFile">
  <items>
    <MenuItem mnemonicParsing="false" onAction="#onEditFile" text="Edit File" />
    <SeparatorMenuItem mnemonicParsing="false" />
    <MenuItem mnemonicParsing="false" onAction="#onDeleteFile" text="Remove Entry" />
